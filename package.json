{
  "name": "vscode-angular-ts",
  "displayName": "vscode-angular-ts",
  "description": "vscode-angular-html syntax highlighting ported to angular-inline templates",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "scopeName": "template.ng",
        "path": "./syntaxes/template.ng.json",
        "injectTo": [
          "source.js",
          "source.ts"
        ]
      }
    ]
  }
}
